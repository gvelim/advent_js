{
    "tasks": {
        "test": "deno run -A vitest --run "
    },
    "imports": {
        "effect": "npm:effect@^3.6.0",
        "vitest": "npm:vitest@^2.0.5"
    },
    "lint": {
        "include": ["src/"],
        "exclude": ["src/testdata/", "src/fixtures/**/*.ts"],
        "rules": {
            "tags": ["recommended"],
            "include": ["ban-untagged-todo"],
            "exclude": ["no-unused-vars"]
        }
    }
}
